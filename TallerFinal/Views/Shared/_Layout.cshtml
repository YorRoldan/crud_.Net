<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>@ViewData["Title"] - IMPORTACIONES SAS</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
 

  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  <link rel="stylesheet" href="~/css/adminlte.min.css">
  <link rel="stylesheet" href="~/js/adminlte.min.js">

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.1.3/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.5/css/dataTables.bootstrap5.min.css" />

</head>
<body class="hold-transition sidebar-mini">
<div class="wrapper">
  <!-- Navbar -->
      <nav class="main-header navbar navbar-expand navbar-white navbar-light">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="bi bi-list"></i></a>
          </li>
          <li class="nav-item d-none d-sm-inline-block">
            <a asp-controller="Principal" asp-action="Inicio" class="nav-link">INICIO</a>
          </li>
          <li class="nav-item d-none d-sm-inline-block">
             <a asp-controller="Principal" asp-action="Help" class="nav-link">HELP</a>
          </li>
        </ul>
      </nav>
      <aside class="main-sidebar sidebar-dark-primary elevation-4">
        <a asp-area="" asp-controller="Principal" asp-action="Inicio" class="brand-link">
          <img src="/Img/LikeFacebok.jpg" alt="Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
          <span class="brand-text font-weight-light">IMPORT-SYSTEM</span>
        </a>

        <div class="sidebar">
          <div class="user-panel mt-3 pb-3 mb-3 d-flex">
            <div class="image">
              <img src="~/Img/FOTO HV 3.jpg" class="img-circle elevation-2" alt="User Image">
            </div>
            <div class="info">
              <a href="#" class="d-block">Yorleidy Roldán</a>
            </div>
          </div>

         
          <nav class="mt-2">
            <ul class="nav nav-treeview">
                <li class="nav-item">
                <a asp-area="" asp-controller="Usuarios" asp-action="ListaDeUsuario" class="nav-link">
                    <i class="bi bi-person-square"></i>
                    Usuarios
                </a>
                </li>
            </ul>
            <ul class="nav nav-treeview">
                <li class="nav-item">
                <a asp-area="" asp-controller="Clientes" asp-action="ListaDeClientes" class="nav-link">
                    <i class="bi bi-people-fill"></i>
                    Clientes
                </a>
                </li>
            </ul>
            <ul class="nav nav-treeview">
                <li class="nav-item">
                <a asp-area="" asp-controller="Proveedores" asp-action="ListaDeProveedores" class="nav-link">
                   <i class="bi bi-building"></i>
                    Proveedores
                </a>
                </li>
            </ul>
            <ul class="nav nav-treeview">
                <li class="nav-item">
                <a asp-area="" asp-controller="Productos" asp-action="ListaDeProductos" class="nav-link">
                   <i class="bi bi-cart-check-fill"></i>
                    Productos
                </a>
                </li>
            </ul>
          </nav>
        </div>
      </aside>
     
      <div class="content-wrapper">
     
        <section class="content-header">
          <div class="container-fluid">
            <div class="row mb-2">
              <div class="col-sm-12">
                <h1 class="text-center">IMPORTACIONES SAS</h1>
              </div>
             </div>
          </div>
        </section>
 
           <section class="content">

                 @RenderBody()            

            </section>
      </div>


      <footer class="main-footer">
        <strong class="text-center">Copyright &copy; 2021-2022 <a href=" ">IMPORTACIONES.SAS</a>.</strong> Derechos Reservados.
      </footer>

 
      <aside class="control-sidebar control-sidebar-dark"></aside>
 
</div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
     
    <script src="~/js/adminlte.min.js"></script>

<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap5.min.js"></script>
    <script src="~/js/site.js"></script>




    

</body>
</html>